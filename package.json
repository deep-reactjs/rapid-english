    "ios": "react-native run-ios",
    "start": "react-native start --port=8081 --reset-cache",
    "test": "jest",
    "lint": "eslint *.js **/*.js",
    "lintfix": "eslint . --fix",
    "generate-apk": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && gradlew.bat assembleDebug",
    "build-android-w": "cd android && gradlew clean && cd .. && react-native run-android --variant=release",
    "build-android-m": "cd ./android && ./gradlew clean && ../ && react-native run-android --variant=release"
